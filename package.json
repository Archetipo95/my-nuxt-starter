{
  "name": "my-nuxt-starter",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "serve-storybook": "npx http-server ./storybook-static",
    "clean-node": "rm -rf node_modules",
    "clean-nuxt": "rm -rf .nuxt",
    "clean-project": "pnpm clean-node && pnpm clean-nuxt && rm -rf auto-imports.d.ts && rm -rf components.d.ts"
  },
  "packageManager": "pnpm@8.15.4",
  "devDependencies": {
    "@formkit/addons": "1.6.0",
    "@formkit/icons": "1.6.0",
    "@formkit/nuxt": "1.6.0",
    "@formkit/pro": "0.122.9",
    "@formkit/themes": "1.6.0",
    "@nuxtjs/tailwindcss": "6.11.4",
    "@storybook/addon-a11y": "8.0.0",
    "@storybook/addon-designs": "8.0.0",
    "@storybook/addon-essentials": "8.0.0",
    "@storybook/addon-interactions": "8.0.0",
    "@storybook/addon-links": "8.0.0",
    "@storybook/addon-themes": "8.0.0",
    "@storybook/test": "8.0.0",
    "@storybook/testing-library": "0.2.2",
    "@storybook/vue3": "8.0.0",
    "@storybook/vue3-vite": "8.0.0",
    "@types/node": "20.11.30",
    "@vueuse/nuxt": "10.9.0",
    "chromatic": "11.0.8",
    "nuxt": "3.10.3",
    "nuxt-headlessui": "1.1.5",
    "nuxt-lodash": "2.5.3",
    "sass": "1.72.0",
    "storybook": "8.0.0",
    "typescript": "5.4.2",
    "unplugin-auto-import": "0.17.5",
    "unplugin-icons": "0.18.5",
    "unplugin-vue-components": "0.26.0",
    "vite-tsconfig-paths": "4.3.2"
  }
}
